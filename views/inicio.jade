    a.cd-top(href="#0", title="Go to top") Top
    // jQuery (necessary for Bootstrap's JavaScript plugins)
    script(src="js/jquery.min.js")
    // Include all compiled plugins (below), or include individual files as needed
    script(src="js/bootstrap.min.js")
    script(src="js/menumaker.js")
    // animsition
    script(type="text/javascript", src="js/animsition.js")
    script(type="text/javascript", src="js/animsition-script.js")
    // sticky header
    script(type="text/javascript", src="js/jquery.sticky.js")
    script(type="text/javascript", src="js/sticky-header.js")
    // testimonial-carousel
    script(type="text/javascript", src="js/owl.carousel.min.js")
    |  
    script(type="text/javascript", src="js/testimonial-carousel.js")
    // Back to top script
    script(src="js/back-to-top.js", type="text/javascript")
